{
    "manifest_version": 2,
    "name": "Only News Feed",
    "description": "Extension removes 'Suggested for you' section from Facebook feed",
    "version": "1",
    "icons": {
        "16": "images/icons8-news-feed-96.png",
        "48": "images/icons8-news-feed-96.png",
        "128": "images/icons8-news-feed-96.png"
    },
    "page_action": {
        "default_icon": {
            "16": "images/icons8-news-feed-96.png",
            "48": "images/icons8-news-feed-96.png",
            "128": "images/icons8-news-feed-96.png"
        }
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["script.js"]
    }]    
}